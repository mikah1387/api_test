{% extends 'base.html.twig' %}

{% block title %}All posts {% endblock %}

{% block body %}


<h1> Nos articles</h1>

<table>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
    <th>Description</th>
    <tbody>
       {% for post in posts %}
       <tr>
        <td>{{ post.id }}</td>
        <td>{{ post.name }}</td>
        <td>{{ post.email }}</td>
        <td>{{ post.body }}</td>
     </tr>
       {% endfor %}
      
    </tbody>
</table>
{% endblock %}
